{
    "name": "openeuropa/task-runner-changelog",
    "description": "Commands for generating and handling the changelog.",
    "keywords": ["task-runner", "changelog"],
    "type": "library",
    "require": {
        "php": "~7.1",
        "openeuropa/task-runner": "dev-ISAICP-6037"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0||~7.0",
        "slevomat/coding-standard": "~6",
        "squizlabs/php_codesniffer": "~3"
    },
    "license": "EUPL-1.2",
    "autoload": {
        "psr-4": {
            "OpenEuropa\\Changelog\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEuropa\\Changelog\\Tests\\": "./tests/",
            "OpenEuropa\\TaskRunner\\Tests\\": "./vendor/openeuropa/task-runner/tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
